diagram(course)
	grid
	  columns for a : course.assignments {
	    col a
	    text [align="center"] a.category.name
	    text [align="center"] a.name
	    text [align="right"]  a.points
	  }
	  rows for s : course.students {
	    row s
	    text [align="right"] s.number
	    text s.name
	    text [align="right"] s.id
	  }    
	  body for g : course.grades {
	    row g.student
	    col g.assignment
  		text [align="right"] g.grade
	  }

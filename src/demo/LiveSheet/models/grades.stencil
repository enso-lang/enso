diagram(course)

	grid
	  columns for a : course.assignments {
	    col a
	    text a.category.name
	    text a.name
	    text a.points
	  }
	  rows for s : course.students {
	    row s
	    text s.number
	    text s.name
	    text s.id
	  }    
	  body for g : course.grades {
	    row g.student
	    col g.assignment
  		text g.grade
	  }
